@article{Wang2013,
abstract = {Benjamin Franklin, mostly known for his participation in writing The Declaration of Independence and work on electricity, was also one of the first scientists to seek to understand the properties of oil monolayers on water surfaces. During one of his many voyages across the Atlantic Ocean, Franklin observed that oil had a calming effect on waves when poured into rough ocean waters. Though at first taking a backseat to many of his other scientific and political endeavors, Franklin went on to experiment with oil, spreading monomolecular films on various bodies of water, and ultimately devised a concept of particle repulsion that is indirectly related to the hydrophobic effect. His early observations inspired others to measure the dimensions of oil monolayers, which eventually led to the formulation of the contemporary lipid bilayer model of the cell membrane. {\textcopyright} 2013 Biophysical Society.},
author = {Wang, Da Neng and Stieglitz, Heather and Marden, Jennifer and Tamm, Lukas K.},
doi = {10.1016/j.bpj.2012.12.028},
file = {:Users/remimetzdorff/Documents/biblio/articles/franklin{\_}membrane{\_}biophysicist.pdf:pdf},
issn = {00063495},
journal = {Biophysical Journal},
keywords = {franklin,inspe},
mendeley-tags = {franklin,inspe},
number = {2},
pages = {287--291},
pmid = {23442850},
publisher = {Biophysical Society},
title = {{Benjamin franklin, philadelphia's favorite son, was a membrane biophysicist}},
url = {http://dx.doi.org/10.1016/j.bpj.2012.12.028},
volume = {104},
year = {2013}
}
