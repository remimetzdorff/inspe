@article{Rayleigh1890,
abstract = {NUMEROUS and varied phenomena prove that the tension of a water surface is lowered by the presence of even a trace of grease. In the case of olive-oil, a film whose calculated thickness is as low as 2 micro-millimetres is sufficient to entirely alter the properties of the surface in relation to fragments of camphor floating thereupon. It seemed to me of importance for the theory of capillarity to ascertain with some approach to precision the tensions of greasy surfaces; and in a recent paper I gave some results applicable to the comparison of a clean surface with one just greasy enough to stop. the camphor movements and also with one saturated with olive-oil. The method employed was that depending upon the rise of liquid between parallel plates of glass ; and I was not satisfied with it, not merely on account of the roughness of the measurement, but also because the interpretation of the result depends upon the assumption that the angle of contact with the glass is zero. In the opinion of Prof. Quineke, whose widely extended researches in this field give great weight to his authority, this assumption is incorrect even in the case of pure liquids, and, as it seemed to me, is still less to be trusted in its application to contaminated surfaces, the behaviour of which is still in many respects obscure. I was thus desirous of checking my results by a method independent of the presence of a solid body.},
author = {Rayleigh, Lord},
doi = {10.1080/14786449008620040},
file = {:Users/remimetzdorff/Documents/biblio/articles/rayleigh{\_}on{\_}the{\_}tension{\_}of{\_}water{\_}surfaces.pdf:pdf},
issn = {1941-5982},
journal = {The London, Edinburgh, and Dublin Philosophical Magazine and Journal of Science},
keywords = {franklin,inspe},
mendeley-tags = {franklin,inspe},
number = {186},
pages = {386--400},
title = {{XLIII. On the tension of water surfaces, clean and contaminated, investigated by the method of ripples}},
volume = {30},
year = {1890}
}
